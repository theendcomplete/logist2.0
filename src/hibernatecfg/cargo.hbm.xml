<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="classes.Cargo" table="cargos">
        <id column="cargo_ID" name="cargo_ID" type="java.lang.Long">
            <generator class="increment"/>
        </id>
        <property column="name" name="name" type="java.lang.String"/>
        <property column="count" name="count" type="java.lang.Integer"/>

        <set name="unit" table="units" lazy="false">
            <key column="unit_ID"/>
            <many-to-many column="unit_ID" class="classes.Unit"/>
        </set>
    </class>
</hibernate-mapping>